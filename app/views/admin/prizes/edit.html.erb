<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <div class="panel panel-default">
        <div class="panel-heading">
          <h1>Editar Premio</h1>
        </div>
        <div class="panel-body">
         <%= render 'form', url: admin_prize_path, title: "Editar"%>
       </div>
        <div class="panel-footer">
          <%= link_to 'Ver', admin_prize_path(@prize), class: "btn btn-default" %>
          <%= link_to 'Regresar', admin_path, class: "btn btn-default" %>
        </div>
    </div>
  </div>
</div>
